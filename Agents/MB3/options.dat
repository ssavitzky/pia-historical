 <!-- default options for MB3 agent -->

 <options>
  <agentoption>
   <optionname>shareDIR</optionname>
    <optionvalue>/C/presentations</optionvalue>
    <optiondescription>The name of the directory where presentation files
    should be stored.  Users should be able to mount this directory and copy
    files into it. \\&piaHOST;  will be prepended  to this value and  "/" will be
    converted into "\" for the giving mount instructions to users. </optiondescription>
  </agentoption>
 <agentoption>
   <optionname>annotationDIR</optionname>
    <optionvalue>/C/sfree </optionvalue>
    <optiondescription>Location for images which are created by the annotation
    program (drawing on the screen). </optiondescription>
  </agentoption>
<agentoption>
   <optionname>calendarURL</optionname>
    <optionvalue>http://internal/</optionvalue>
    <optiondescription>URL of office calendar (used for creating links to the
    local community calendar, could eventually automatically load entries from
    this calendar into the MB3 calendar.)</optiondescription>
  </agentoption> 

 </options>
