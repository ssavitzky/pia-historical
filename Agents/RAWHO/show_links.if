<!doctype html public "-//IETF//DTD HTML//EN//2.0">
<html><head>
<title> show links</title>
<link rev="made" href="mailto:wolff@crc.ricoh.com"></link>
</head><body>
<read file=heading.inc interform process>

<form action="&url;" method=post id="showlinks" copy>
<input type="SUBMIT" value="ShowLinksIn">
:<select name="categories" multiple>
 <set name=k><get agent index=cats keys></set>	      
 <repeat list=&k;> <option>&li;</option></repeat>	      
 </select>
 <process copy>
<set name=links><dl> </dl></set>
 <repeat list=&FORM.categories; entity=catname>
 category  &catname; links:
  <set name=items><get agent index=cats.&catname; key="links"></set>
   <repeat list=&items; entity=l>
    <set name=urlid><get index="l.a" attr=urlid></set>
    <set  index=links key=&urlid;>&catname;</set>
     </repeat>
 </repeat>
  <table>
<TR><TH>linkid<TH>description<TH>categories</tr>
   <repeat list=&links.keys; entity=ky>
    <TR><TD align=right> <get name=ky> <TD><get agent index=links..dl-1 key=&ky;>
        <TD align=left> <get name=links key=&ky;> </td></tr>
   </repeat>
  </table>
 </process>

</form>


