<!doctype html public "-//IETF//DTD HTML//EN//2.0">
<html><head>
<title> WebPrinter preview</title>
<link rev="made" href="mailto:wolff@crc.ricoh.com">
</head><body>
<h1><a href="/agency/home.if"></a>`<a href="home.if">&agentName;</a>' on &piaHOST
</h1>
<if><test><get form name="URL"></test>
<then>
Previewing &FORM.URL;
<p>

<code language=perl>
my %form_parameters=%{$request->parameters()};
$agent->option('thumbsize',$form_parameters{thumbsize}) if exists $form_parameters{thumbsize};
$agent->option('render_method',$form_parameters{render_method}) if exists $form_parameters{render_method};

</code>

<code language=perl>
my $self=$agent;print $agent->name . ref($agent) . " is our name andtype\n";
my %form_parameters=%{$request->parameters()};
my $ask=$self->create_request('GET',$form_parameters{URL});
my $element=$self->create_preview($ask);

print "preview created \n";
return $element;

</code>
 <form action="/&agentName;/print.if" method=post>
<input name="URL" value="&FORM.URL;" type=hidden>
<input name="spoolfile" value="&AGENT.ps_file_name;" type=hidden>
<input type="SUBMIT" value="Print">
</form>

<form action="/&agentName;/preview.if" method=post>
<input name="URL" value="&FORM.URL;" type=hidden>
<input name="thumbsize" value="30" type=hidden>
<input type="SUBMIT" value="SmallThumbnails">
</form>
<form action="/&agentName;/preview.if" method=post>
<input name="URL" value="&FORM.URL;" type=hidden>
<input name="render_method" value="normal" type=hidden>
<input type="SUBMIT" value="NoImages">
</form>


</then>
<else>
<form action="&url;" method=POST>
<set name="myurl"><get trans headers request name=referer></set>
<input type="URL" name="URL" value=&myurl;><input type="SUBMIT" value="Preview">
</form>
</else>
</if>
