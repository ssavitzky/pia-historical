<h1>Printer initialization for <code language=perl> $current_self->name; </code></h1>

<code language=perl>

$current_self->option('render_method',"latex");
$current_self->option('preview_number',"100");
$current_self->option('thumbsize',"72");
$current_self->option('thumb_width',"100");
$current_self->option('thumb_height',"140");
$current_self->option('book_depth',"2");
$current_self->option('cache_only',"1");
$current_self->option('max_pages',"2000");
$current_self->option('device',"lp");

my $name= $current_self->name;
my $printer_root_directory=$current_self->agent_directory . "spool/";
$current_self->option('printer_root_directory',$printer_root_directory);

mkdir($printer_root_directory, 0777) unless -d $printer_root_directory;

$self->option('printer_base_url',$current_self->agent_url . "spool/");

"initialized $name";
</code>




