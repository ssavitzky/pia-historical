<!doctype html public "-//IETF//DTD HTML//EN//2.0">
<html><head>
<title>&agentName; URL Printing</title>
<link rev="made" href="mailto:wolff@crc.ricoh.com"></link>
</head><body>
<set entity name=title>Printing</set>
<!-- put up the header at the top of the page -->
<read interform file="heading.inc" process>

<tagset-load name=Printer-agent-actors>

<if> <get form name=spoolfile> <then>
 


<print-ps-file psfile=&usrDIR;/&agentName;/&FORM.spoolfile;>
&FORM.spoolfile; is being printed on the WebPrinter.
<hr>

 </then> <else>
<if> <get name=url form> <then>
 <set name="theurl"><get name="url" form></set>
 <if><get form name=style> <then> <set name=style><get form
 name=style></set></then> <else>
<set name=style><default-style url=&theurl;></set> 
 </else></if>

 <set name=spoolfile><new-file-name></set>

    <fetch-url input=&theurl; style=&thisstyle; output=&spoolfile;>

    <set name=commandout> </set>
<!--     creating Postscript...using style &thisstyle; -->

    <create-ps-file-simple file=&spoolfile;.html input=&theurl;
    output=&spoolfile; style=&thisstyle;>

<print-ps-file psfile=&usrDIR;/&agentName;/&spoolfile;.ps>
&theurl; is being printed on the WebPrinter.
</then>

<else>


<form action="&url;" method=POST>
<set name="refered"><get trans headers request name=referer></set>
<if><test match=&piaHOST;><get name=refered></test>
 <else><set name=myurl><get name=refered></set> </else>
 </if>
  <if>
  <test><get name="myurl"></test>
  <else>
  <set-lastlink>
  <set name="myurl"><get name="lastlink" attr=href></set>
   </else>
 </if>

Format this URL: <input type="URL" size=65 name="url" value=&myurl;>

<br>
Using this <a href=edit_style.if>Style:</a>
<select name=style>
 <style-option-list url=&myurl;>
</select>
<input type="SUBMIT" value="Print">
</form>


 </else> </if></else> </if>

<hr>
<page-footer> </page-footer>
</body></html>
