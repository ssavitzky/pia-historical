for argument in `ls -l users | awk '{print $9}' | grep -v complete`
do
   if (test `wc -w users/$argument | awk '$1 == 0' | awk '{print $2}'`)
   then rm users/$argument
   fi
done

for argument in `ls -l users | awk '{print $9}' | grep -v complete`
do
   cat message.txt | mail `echo $argument | awk 'BEGIN {FS = "."} {print $1}'`
done
