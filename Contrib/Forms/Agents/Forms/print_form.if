<!doctype html public "-//IETF//DTD HTML//EN//2.0">
<html><head>
<title>&agentName; printing</title>
<link rev="made" href="mailto:wolff@crc.ricoh.com"></link>
</head><body>
 <set name=myformurl><get form name=formurl></set>
<set entity name=title> Printing <get name=myformurl></set>
<read interform file="heading.inc" process>
 
<tagset-load name=Form-agent-actors>
 <set name=user><get trans name=AuthenticatedUser></set>
 <set name=printfile>printouts/&user;.&day;.&time;.&second;.html</set>
<!-- create static html file to be printed -->

 <write file=&printfile;><read interform file="show_signatures_simple.if" process></write>
 <if> <test match=other><get form name=printer></test>
 <then> <set name="printer"><get form name=otherprinter></set></then>
 <else> <set name="printer"><get form name=printer></set></else>
</if>
<if><get name=printer> <then>
 Printing &myformurl; on &printer; for &user;
<set name=success><read href="http://salmon.crc.ricoh.com:8001/cgi-bin/imws.cgi"
query="username=&user;?src_url=http://&piaHOST;:&piaPORT;/My/&agentName;/&printfile;?printer=&printer;"></set>

<hr>
         <show-pending user=&user;>
         <repeat list=&AGENT.groups.keys; entity=mygrpo>
         <if-in-group user=&FORM.user; group=&mygrpo;>
           <show-pending user=&mygrpo;>
         </if-in-group>
         </repeat>
	</then>
<hr>
</then> <else> No printer specified, please go back and specified a
	printer.</else> </if>

<if><get name=success><else>If an error occurs...<form
action="panic.if" method=post> Hit the 
<input name="form" type=hidden value=&myformurl;>
<input name="printer" type=hidden value=&printer;>
<input name="user" type=hidden value=&user;>
<input type="SUBMIT" value="PanicButton">
</form>
</else></if>
<expand><get agent name=navbar></expand>
<b>Copyright © 1998 Ricoh Silicon Valley</b><br>
<!-- $Id$ -->
</body></html>
